{
  "name": "shapes",
  "version": "0.4.0",
  "description": "A shapes library for JavaScript",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && node es5-generated/test/test.js",
    "test-coveralls": "npm run istanbul && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "rm -rf coverage && npm run istanbul && istanbul report html && serve coverage",
    "istanbul": "npm run build && istanbul cover es5-generated/test/test.js",
    "lint": "eslint src-es6/* && jshint src-es6/*",
    "build": "rm -rf es5-generated && mkdir es5-generated  && babel src-es6 --out-dir es5-generated",
    "test-manually": "beefy es5-generated/test/manually.js -v",
    "test-manually-live": "beefy es5-generated/test/manually.js -v --live"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tillarnold/shapes"
  },
  "keywords": [
    "shapes",
    "canvas"
  ],
  "author": "Till Arnold",
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.5.3",
    "tape": "^3.0.0",
    "istanbul": "~0.3.0",
    "coveralls": "~2.11.1",
    "eslint": "^0.15.0",
    "beefy": "^2.1.1",
    "watchify": "^2.1.0",
    "babel": "^4.6.0",
    "babel-eslint": "^1.0.6"
  },
  "dependencies": {
    "canvas-utils": "^0.6.0",
    "es6-map": "^0.1.1",
    "red-blue-line-segment-intersect": "^1.0.1"
  }
}
